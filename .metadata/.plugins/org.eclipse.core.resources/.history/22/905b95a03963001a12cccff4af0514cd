#include "my_it_callbacks.h"

uint8_t dat[4] = {'o','k','\r','\n'};


void HAL_UART_RxCpltCallback (UART_HandleTypeDef *huart)
{
  uint8_t buffer_2[10];
 if(huart->Instance == USART2)
 {
   HAL_GPIO_TogglePin(GPIOA,LD2_Pin);
   HAL_UART_Receive_IT(&huart2, buffer_2, 10);
 }

}

