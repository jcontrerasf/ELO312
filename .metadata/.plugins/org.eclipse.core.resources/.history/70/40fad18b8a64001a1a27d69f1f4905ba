#include "my_it_callbacks.h"

uint8_t interrupciones = 0;
static uint8_t switches = 0;

void HAL_GPIO_EXTI_Callback(uint16_t GPIO_Pin)
{


  if(GPIO_Pin == enviar_Pin)
  {
    interrupciones++;
    switches |= HAL_GPIO_ReadPin(GPIOC,switch1_Pin);
    switches |= HAL_GPIO_ReadPin(GPIOC,switch2_Pin)>>1;
    switches |= HAL_GPIO_ReadPin(GPIOC,switch3_Pin)>>2;
  }

}

