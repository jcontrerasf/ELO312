#include "my_it_callbacks.h"
#include "stdint.h"
#include "gpio.h"
#include "main.h"

void HAL_GPIO_EXTI_Callback(uint16_t GPIO_Pin)
{
  if(GPIO_Pin == in1_Pin)
  {
    funcion_change(HAL_GPIO_ReadPin(GPIOA, in1_Pin));
  }
}

